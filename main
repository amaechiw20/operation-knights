import sys
import pygame

pygame.init()

width = 800
heigh = 600

screen = pygame.display.set_mode((width,height))

character = pygame.image.load("IMAGE")
charRect = character.get_rect()

game_over = False

while not game_over:
    for event in pygame.event.get():
        if event.type == pygame.QUIT:
            sys.exit()
            
    screen.fill(0,0,0)
    screen.blit()
    pygame.display.flip()
